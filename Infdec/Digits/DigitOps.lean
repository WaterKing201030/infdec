theorem half_add3.snd_translation'{a b c d e:Digit}(h:(half_add3 a b c).snd = (half_add3 d b e).snd)(f:Digit):(half_add3 a f c).snd = (half_add3 d f e).snd:=by{
  rw[comm23 a _ c, comm23 d _ e] at *
  exact snd_translation'' h f
}

theorem half_add3.snd_translation{a b c d e:Digit}(h:(half_add3 a b c).snd = (half_add3 a d e).snd)(f:Digit):(half_add3 f b c).snd = (half_add3 f d e).snd:=by{
  rw[comm13 _ b c, comm13 _ d e] at *
  exact snd_translation'' h f
}

theorem half_add3.translation'{a b c d e:Digit}(h:half_add3 a b c = half_add3 d b e)(f:Digit):half_add3 a f c = half_add3 d f e:=
  ((comm23 a f c).trans (translation'' (((comm23 a c b).trans h).trans (comm23 d b e)) f)).trans (comm23 d e f)

theorem half_add3.translation{a b c d e:Digit}(h:half_add3 a b c = half_add3 a d e)(f:Digit):half_add3 f b c = half_add3 f d e:=
  ((comm13 f b c).trans (translation'' (((comm13 c b a).trans h).trans (comm13 a d e)) f)).trans (comm13 e d f)

theorem half_add3.one_one_eq_zero_two(a:Digit):half_add3 a (1) (1) = half_add3 a (0) (2):=
  match a with
  | (0)
  | (1)
  | (2) => rfl
