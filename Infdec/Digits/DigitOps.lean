theorem half_add3.snd_cancel'{x y z w:Digit}(h:(half_add3 x y w).snd=(half_add3 x z w).snd):y = z:=by{
  rw[comm23 x y w, comm23 x z w] at h
  exact half_add3.snd_cancel'' h
}

theorem half_add3.snd_cancel{x y z w:Digit}(h:(half_add3 x z w).snd=(half_add3 y z w).snd):x = y:=by{
  rw[comm13 x z w, comm13 y z w] at h
  exact snd_cancel'' h
}

theorem half_add3.cancel''{x y z w:Digit}(h:half_add3 x y z=half_add3 x y w):z = w:=by{
  have : (half_add3 x y z).snd = (half_add3 x y w).snd := by simp[h]
  exact snd_cancel'' this
}

theorem half_add3.cancel'{x y z w:Digit}(h:half_add3 x y w=half_add3 x z w):y = z:=
  cancel'' (((comm23 x w y).trans h).trans (comm23 x z w))

theorem half_add3.cancel{x y z w:Digit}(h:half_add3 x z w=half_add3 y z w):x = y:=
  cancel'' (((comm13 w z x).trans h).trans (comm13 y z w))

